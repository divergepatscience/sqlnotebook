/*
A1
A2
A3
B1
B2
B3
C3
C2
C1
D1
D2
D3
*/

FOR @i = 1 TO 3
    PRINT 'A' || @i;

FOR @i = 1 TO 10 BEGIN
    PRINT 'B' || @i;
    IF @i = 3
        BREAK;
END

FOR @i = 3 TO 1 STEP -1
    PRINT 'C' || @i;

DECLARE @a = 1;
DECLARE @b = 3;
FOR @i = @a TO @b BEGIN
    PRINT 'D' || @i;
    SET @b = 10; -- does NOT affect the loop
END
