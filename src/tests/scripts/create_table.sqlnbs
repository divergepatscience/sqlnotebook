PRINT "2. " || (SELECT COUNT(*) FROM aaa);
PRINT "3. " || (SELECT COUNT(*) FROM sqlite_master);

DROP TABLE aaa;

PRINT "4. " || (SELECT COUNT(*) FROM sqlite_master);
