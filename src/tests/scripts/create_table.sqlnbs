CREATE TABLE aaa (
    a INTEGER NOT NULL,
    b TEXT NOT NULL,
    c REAL NOT NULL,
    d INTEGER NULL,
    PRIMARY KEY (a)
);

PRINT "1. " || (SELECT COUNT(*) FROM aaa);

